/* - Створити змінні. Присвоїти кожному з них значення: 'hello','owu','com', 'ua', 1, 10, -999, 123, 3.14, 2.7, 16, true, false.
  Вивести кожну змінну за допомогою: console.log , alert, document.write */

let a = 'hello';
console.log(a);
alert(a);
document.write(a);
let b = 'owu';
console.log(b);
alert(b);
document.write(b);
let c = 'com';
console.log(c);
alert(c);
document.write(c);
let d = 'ua';
console.log(d);
alert(d);
document.write(d);
let e = 1;
console.log(e);
alert(e);
document.write(e);
let f = 10;
console.log(f);
alert(f);
document.write(f);
let g = -999;
console.log(g);
alert(g);
document.write(g);
let h = 123;
console.log(h);
alert(h);
document.write(h);
let i = 3.14;
console.log(i);
alert(i);
document.write(i);
let k = 2.7;
console.log(k);
alert(k);
document.write(k);
let l = 16;
console.log(l);
alert(l);
document.write(l);
let m = true;
console.log(m);
alert(m);
document.write(m);
let n = false;
console.log(n);
alert(n);
document.write(n);


/* - Переприсвоїти кожній змінній з завдання значення на довільне.
  Вивести кожну змінну за допомогою: console.log , alert, document.write
 */

let a = 'hello';
a = 'Hi';
console.log(a);
alert(a);
document.write(a);
let b = 'owu';
b = 'www';
console.log(b);
alert(b);
document.write(b);
let c = 'com';
c = 'net';
console.log(c);
alert(c);
document.write(c);
let d = 'ua';
d = 'rt';
console.log(d);
alert(d);
document.write(d);
let e = 1;
e = 2;
console.log(e);
alert(e);
document.write(e);
let f = 10;
f = 15;
console.log(f);
alert(f);
document.write(f);
let g = -999;
g = 'rrr';
console.log(g);
alert(g);
document.write(g);
let h = 123;
h = 535346;
console.log(h);
alert(h);
document.write(h);
let i = 3.14;
i = 'hello';
console.log(i);
alert(i);
document.write(i);
let k = 2.7;
k = 23.99;
console.log(k);
alert(k);
document.write(k);
let l = 16;
l = 'eee';
console.log(l);
alert(l);
document.write(l);
let m = true;
m = false;
console.log(m);
alert(m);
document.write(m);
let n = false;
n = true;
console.log(n);
alert(n);
document.write(n);
